	<head>
		<title>Detail_bcont</title>
		<script>
			function handleButPrsViewClick() {parent.parent.window.handleButCrdopenClick("ButPrsView");};
			function handleButUsgViewClick() {parent.parent.window.handleButCrdopenClick("ButUsgView");};
			function handlePupSteChange() {parent.parent.window.handlePupChange(document,"PupSte","");};
			function handlePupLclChange() {parent.parent.window.handlePupChange(document,"PupLcl","");};
			function handlePupUlvChange() {parent.parent.window.handlePupChange(document,"PupUlv","");};
			function handleTxfPwdKey(evt) {return parent.parent.window.handleTxfKey(document,"TxfPwd","s",evt);};
			function handleTxfPwdChange() {parent.parent.window.handleTxfChange(document,"TxfPwd","s");};
			function handleTxfFkyKey(evt) {return parent.parent.window.handleTxftKey(document,"TxfFky",evt);};
			function handleTxfFkyChange() {parent.parent.window.handleTxftChange(document,"TxfFky");};
			function handleTxfCmtKey(evt) {return parent.parent.window.handleTxftKey(document,"TxfCmt",evt);};
			function handleTxfCmtChange() {parent.parent.window.handleTxftChange(document,"TxfCmt");};
			<!-- IP custScript - INSERT -->
		</script>
		<link href="../style/pnlDetail.css" rel="styleSheet" type="text/css"/>
		<!-- IP custStyle - INSERT -->
	</head>
