# file Makefile
# make file for CrdWdbeMod card job handler, release wdbed_jack
# author Alexander Wirthmueller
# date created: 23 Aug 2020
# modified: 23 Aug 2020

include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread -DWDBED

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I$(SRCROOT)/dbswdbe -I$(SRCROOT)/wdbed/VecWdbe -I$(SRCROOT)/wdbed/IexWdbe -I$(SRCROOT)/wdbed
INCPATH += -I/home/mpsitech/src/apiwzlm -I/usr/include -I/usr/include/jsoncpp -I/usr/include/libxml2 -I/usr/include/mysql -I/usr/include/postgresql -I/usr/local/include

OBJS = CrdWdbeMod.o PnlWdbeModRec.o PnlWdbeModDetail.o PnlWdbeModKHdltype.o QryWdbeModKHdltype.o PnlWdbeModAPar.o QryWdbeModAPar.o PnlWdbeModMdl1NGeneric.o QryWdbeModMdl1NGeneric.o PnlWdbeModCor1NImbuf.o QryWdbeModCor1NImbuf.o PnlWdbeModMdl1NPort.o QryWdbeModMdl1NPort.o PnlWdbeMod1NProcess.o QryWdbeMod1NProcess.o PnlWdbeModMdl1NSignal.o QryWdbeModMdl1NSignal.o PnlWdbeMod1NPeripheral.o QryWdbeMod1NPeripheral.o PnlWdbeModSup1NModule.o QryWdbeModSup1NModule.o PnlWdbeModMge1NSignal.o QryWdbeModMge1NSignal.o PnlWdbeModCtrRef1NCommand.o QryWdbeModCtrRef1NCommand.o PnlWdbeModCtrHk1NVector.o QryWdbeModCtrHk1NVector.o PnlWdbeModCtrRef1NError.o QryWdbeModCtrRef1NError.o PnlWdbeModCtrMNCommand.o QryWdbeModCtrMNCommand.o PnlWdbeModHeadbar.o PnlWdbeModList.o QryWdbeModList.o

LIB = libcrdwdbemod

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a $(LIBROOT)/wdbed/

clean:
	rm -f *.o

