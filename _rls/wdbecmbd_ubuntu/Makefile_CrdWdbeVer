include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread -DWDBECMBD

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I$(BUILDROOT)/dbswdbe -I$(BUILDROOT)/wdbecmbd/VecWdbe -I$(BUILDROOT)/wdbecmbd/IexWdbe -I$(BUILDROOT)/wdbecmbd
INCPATH += -I${WHIZSDKROOT}/build -I${WHIZSDKROOT}/build/apiwzlm -I/usr/include -I/usr/include/jsoncpp -I/usr/include/libxml2 -I/usr/include/mariadb

OBJS = CrdWdbeVer.o DlgWdbeVerCmdset.o DlgWdbeVerCustfst.o DlgWdbeVerDflalg.o DlgWdbeVerFinmod.o DlgWdbeVerGenfst.o DlgWdbeVerMdlstr.o DlgWdbeVerNew.o PnlWdbeVerRec.o PnlWdbeVerDetail.o PnlWdbeVer1NRelease.o QryWdbeVer1NRelease.o PnlWdbeVerBvr1NVersion.o QryWdbeVerBvr1NVersion.o PnlWdbeVer1NSystem.o QryWdbeVer1NSystem.o PnlWdbeVer1NUnit.o QryWdbeVer1NUnit.o PnlWdbeVerRef1NFile.o QryWdbeVerRef1NFile.o PnlWdbeVerMNLibrary.o QryWdbeVerMNLibrary.o PnlWdbeVerHeadbar.o PnlWdbeVerList.o QryWdbeVerList.o

LIB = libcrdwdbever

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a $(LIBROOT)/wdbecmbd/

clean:
	rm -f *.o
