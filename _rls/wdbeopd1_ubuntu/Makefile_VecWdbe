include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I${WHIZSDKROOT}/build -I/usr/include -I/usr/include/libxml2 -I/usr/include/mariadb

OBJS = VecWdbeVBasereptype.o VecWdbeVCall.o VecWdbeVDpch.o VecWdbeVError.o VecWdbeVExpstate.o VecWdbeVIop.o VecWdbeVJob.o VecWdbeVMimetype.o VecWdbeVOpengtype.o VecWdbeVOppack.o VecWdbeVQrystate.o VecWdbeVReqitmode.o VecWdbeVStub.o VecWdbeVTag.o

LIB = libvecwdbe

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a $(LIBROOT)/

clean:
	rm -f *.o
