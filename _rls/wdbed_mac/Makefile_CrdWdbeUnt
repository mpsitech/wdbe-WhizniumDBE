include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread -DWDBED

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I$(BUILDROOT)/dbswdbe -I$(BUILDROOT)/wdbed/VecWdbe -I$(BUILDROOT)/wdbed/IexWdbe -I$(BUILDROOT)/wdbed
INCPATH += -I${WHIZSDKROOT}/build -I${WHIZSDKROOT}/build/apiwzlm -I/opt/homebrew/include -I/opt/homebrew/opt/curl/include -I/opt/homebrew/opt/libgit2/include -I/opt/homebrew/opt/libmicrohttpd/include -I/opt/homebrew/opt/libxml2/include -I/opt/homebrew/opt/openssl@3/include -I/opt/homebrew/opt/sqlite3/include -I/usr/local/mysql/include

OBJS = CrdWdbeUnt.o PnlWdbeUntList.o QryWdbeUntList.o PnlWdbeUntHeadbar.o PnlWdbeUntRec.o PnlWdbeUntRef1NCommand.o QryWdbeUntRef1NCommand.o PnlWdbeUntHk1NModule.o QryWdbeUntHk1NModule.o PnlWdbeUntHk1NVector.o QryWdbeUntHk1NVector.o PnlWdbeUntRef1NError.o QryWdbeUntRef1NError.o PnlWdbeUntRef1NSignal.o QryWdbeUntRef1NSignal.o PnlWdbeUnt1NBank.o QryWdbeUnt1NBank.o PnlWdbeUntFwd1NController.o QryWdbeUntFwd1NController.o PnlWdbeUnt1NTarget.o QryWdbeUnt1NTarget.o PnlWdbeUntSil1NUnit.o QryWdbeUntSil1NUnit.o PnlWdbeUnt1NPeripheral.o QryWdbeUnt1NPeripheral.o PnlWdbeUnt1NInterrupt.o QryWdbeUnt1NInterrupt.o PnlWdbeUntDetail.o

LIB = libcrdwdbeunt

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a $(LIBROOT)/wdbed/

clean:
	rm -f *.o
