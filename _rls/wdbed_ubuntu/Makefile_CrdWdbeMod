include ../Makefile.inc

CPP = g++
CPPFLAGS = -std=c++11 -g -Wall -Os -pthread -DWDBED

STATLIB = ar
STATLIBFLAGS = rcs

INCPATH = -I$(BUILDROOT)/dbswdbe -I$(BUILDROOT)/wdbed/VecWdbe -I$(BUILDROOT)/wdbed/IexWdbe -I$(BUILDROOT)/wdbed
INCPATH += -I${WHIZSDKROOT}/build -I${WHIZSDKROOT}/build/apiwzlm -I/usr/include -I/usr/include/jsoncpp -I/usr/include/libxml2 -I/usr/include/mariadb

OBJS = CrdWdbeMod.o PnlWdbeModList.o QryWdbeModList.o PnlWdbeModHeadbar.o PnlWdbeModRec.o PnlWdbeModCtrMNCommand.o QryWdbeModCtrMNCommand.o PnlWdbeModCorMNModule.o QryWdbeModCorMNModule.o PnlWdbeModCtdMNModule.o QryWdbeModCtdMNModule.o PnlWdbeModMNPeripheral.o QryWdbeModMNPeripheral.o PnlWdbeModCtrRef1NCommand.o QryWdbeModCtrRef1NCommand.o PnlWdbeModCtrHk1NVector.o QryWdbeModCtrHk1NVector.o PnlWdbeModCtrRef1NError.o QryWdbeModCtrRef1NError.o PnlWdbeModMge1NSignal.o QryWdbeModMge1NSignal.o PnlWdbeModRef1NSensitivity.o QryWdbeModRef1NSensitivity.o PnlWdbeModRef1NSignal.o QryWdbeModRef1NSignal.o PnlWdbeModRef1NVariable.o QryWdbeModRef1NVariable.o PnlWdbeModSup1NModule.o QryWdbeModSup1NModule.o PnlWdbeModMdl1NGeneric.o QryWdbeModMdl1NGeneric.o PnlWdbeModHsm1NPipeline.o QryWdbeModHsm1NPipeline.o PnlWdbeMod1NCdc.o QryWdbeMod1NCdc.o PnlWdbeModMdl1NPort.o QryWdbeModMdl1NPort.o PnlWdbeMod1NProcess.o QryWdbeMod1NProcess.o PnlWdbeModAPar.o QryWdbeModAPar.o PnlWdbeModKHdltype.o QryWdbeModKHdltype.o PnlWdbeModDetail.o

LIB = libcrdwdbemod

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)

install:
	mv $(LIB).a $(LIBROOT)/wdbed/

clean:
	rm -f *.o
